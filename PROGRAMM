#include <stdio.h>
#include <unistd.h>
void prime_numbers(int n) 
{
    int i, j, isPrime;
    for (i = 2; i <= n; i++) 
	{
        isPrime = 1;
        for (j = 2; j <= i / 2; j++) 
		{
            if (i % j == 0) 
			{
                isPrime = 0;
                break;
            }
        }
        if (isPrime == 1) 
		{
            printf("%d ", i);
        }
    }
}
void fibonacci_series(int n) 
{
    int i, f1 = 0, f2 = 1, next;
    for (i = 0; i < n; i++) 
	{
        if (i <= 1) 
		{
            next = i;
        } else {
            next = f1 + f2;
            f1 = f2;
            f2 = next;
        }
        printf("%d ", next);
    }
}
int main()
{
    pid_t pid;
    int n = 20;
	pid = fork();
    if (pid == 0) 
	{
        fibonacci_series(n);
    } 
	else 
	{
        wait(NULL);
        printf("\nParent: Prime numbers less than or equal to %d\n", n);
        prime_numbers(n);
    }
    return 0;
}
